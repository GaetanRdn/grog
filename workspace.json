{
  "version": 2,
  "projects": {
    "atoms-buttons": {
      "projectType": "library",
      "root": "libs/atoms/buttons",
      "sourceRoot": "libs/atoms/buttons/src",
      "prefix": "gro",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/atoms/buttons"],
          "options": {
            "project": "libs/atoms/buttons/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/atoms/buttons/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/atoms/buttons/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/atoms/buttons"],
          "options": {
            "jestConfig": "libs/atoms/buttons/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/atoms/buttons/src/**/*.ts",
              "libs/atoms/buttons/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/atoms/buttons/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/atoms-buttons",
            "config": {
              "configFolder": "libs/atoms/buttons/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["styles", "tests"]
    },
    "atoms-forms": {
      "projectType": "library",
      "root": "libs/atoms/forms",
      "sourceRoot": "libs/atoms/forms/src",
      "prefix": "gro",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/atoms/forms"],
          "options": {
            "project": "libs/atoms/forms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/atoms/forms/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/atoms/forms/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/atoms/forms"],
          "options": {
            "jestConfig": "libs/atoms/forms/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/atoms/forms/src/**/*.ts",
              "libs/atoms/forms/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/atoms/forms/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/atoms-forms",
            "config": {
              "configFolder": "libs/atoms/forms/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["styles", "tests"]
    },
    "decorators": {
      "projectType": "library",
      "root": "libs/decorators",
      "sourceRoot": "libs/decorators/src",
      "prefix": "gro",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/decorators"],
          "options": {
            "project": "libs/decorators/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/decorators/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/decorators/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/decorators"],
          "options": {
            "jestConfig": "libs/decorators/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/decorators/src/**/*.ts",
              "libs/decorators/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "storybook": {
      "projectType": "application",
      "root": "apps/storybook",
      "sourceRoot": "apps/storybook/src",
      "prefix": "grorg",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/storybook",
            "index": "apps/storybook/src/index.html",
            "main": "apps/storybook/src/main.ts",
            "polyfills": "apps/storybook/src/polyfills.ts",
            "tsConfig": "apps/storybook/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/storybook/src/favicon.ico",
              "apps/storybook/src/assets"
            ],
            "styles": [
              "apps/storybook/src/assets/styles.scss",
              "libs/styles/src/lib/theme.scss",
              "libs/styles/src/lib/utils.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["src/assets/styles", "libs/styles/src/lib"]
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/storybook/src/environments/environment.ts",
                  "with": "apps/storybook/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "storybook:build:production"
            },
            "development": {
              "browserTarget": "storybook:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "storybook:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/storybook/src/**/*.ts",
              "apps/storybook/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "apps/storybook/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/storybook",
            "config": {
              "configFolder": "apps/storybook/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "compodoc": {
          "executor": "@twittwer/compodoc:compodoc",
          "options": {
            "tsConfig": "apps/storybook/tsconfig.compodoc.json",
            "outputPath": "dist/compodoc/storybook",
            "workspaceDocs": true
          },
          "configurations": {
            "json": {
              "exportFormat": "json"
            }
          }
        }
      },
      "tags": []
    },
    "styles": {
      "projectType": "library",
      "root": "libs/styles",
      "sourceRoot": "libs/styles/src",
      "prefix": "gro",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/styles"],
          "options": {
            "project": "libs/styles/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/styles/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/styles/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/styles"],
          "options": {
            "jestConfig": "libs/styles/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
