      }
    },
    "styles": {
      "projectType": "library",
      "root": "libs/styles",
      "sourceRoot": "libs/styles/src",
      "prefix": "grorg",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
