{
  "version": 2,
  "projects": {
    "atoms-buttons": {
      "projectType": "library",
      "root": "libs/atoms/buttons",
      "sourceRoot": "libs/atoms/buttons/src",
      "prefix": "grorg",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/atoms/buttons"],
          "options": {
            "project": "libs/atoms/buttons/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/atoms/buttons/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/atoms/buttons/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/atoms/buttons"],
          "options": {
            "jestConfig": "libs/atoms/buttons/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/atoms/buttons/src/**/*.ts",
              "libs/atoms/buttons/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/atoms/buttons/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/atoms-buttons",
            "config": {
              "configFolder": "libs/atoms/buttons/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "atoms-buttons-e2e": {
      "root": "apps/atoms-buttons-e2e",
      "sourceRoot": "apps/atoms-buttons-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/atoms-buttons-e2e/cypress.json",
            "devServerTarget": "atoms-buttons:storybook",
            "tsConfig": "apps/atoms-buttons-e2e/tsconfig.json"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "atoms-buttons:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/atoms-buttons-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "atoms-forms": {
      "projectType": "library",
      "root": "libs/atoms/forms",
      "sourceRoot": "libs/atoms/forms/src",
      "prefix": "grorg",
      "targets": {
