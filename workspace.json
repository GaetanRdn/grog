{
  "version": 2,
  "projects": {
    "atoms-buttons": "libs/atoms/buttons",
    "atoms-forms": "libs/atoms/forms",
    "decorators": "libs/decorators",
    "storybook": "apps/storybook",
    "styles": "libs/styles",
    "tests": "libs/tests",
    "types": "libs/types"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "decorators"
}
