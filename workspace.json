            "lintFilePatterns": [
              "libs/styles/src/**/*.ts",
              "libs/styles/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "tests": {
      "projectType": "library",
      "root": "libs/tests",
      "sourceRoot": "libs/tests/src",
      "prefix": "gro",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
