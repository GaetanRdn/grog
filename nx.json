{
  "npmScope": "grorg",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "atoms-buttons": {
      "tags": []
    },
    "atoms-buttons-e2e": {
      "tags": [],
      "implicitDependencies": ["atoms-buttons"]
    },
    "atoms-forms": {
      "tags": []
    },
    "decorators": {
      "tags": []
    },
    "styles": {
      "tags": []
    },
    "tests": {
      "tags": []
    }
  }
}
